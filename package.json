{
	"name": "react-interactive-dashboard-static",
	"version": "1.0.0",
	"description": "React Interactive Dashboard is a project I'm doing to level up my frontend skills",
	"scripts": {
		"start:server": "webpack-dev-server --mode development",
		"start:db": "json-server --watch ./src/db/db.json",
		"build": "webpack --mode production",
		"prettier": "prettier \"**/*.+(js|jsx|json|css|scss|ts|tsx|md|mdx|graphql)\"",
		"format": "npm run prettier -- --write",
		"lint": "eslint . --ext .js, .ts, .tsx --fix --no-error-on-unmatched-pattern",
		"types": "tsc --composite false",
		"test": "jest",
		"test:watch": "jest --watch",
		"pre-commit": "lint-staged",
		"validate": "npm-run-all --parallel pre-commit lint format types"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		".+(js|jsx|ts|tsx)": [
			"eslint --fix"
		],
		"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
			"prettier --write",
			"jest --findRelatedTests"
		]
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:georgebullock/react-interactive-dashboard-static.git"
	},
	"keywords": [
		"html",
		"css",
		"javascript",
		"webpack",
		"typescript",
		"jest"
	],
	"author": "George Bullock",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/georgebullock/react-interactive-dashboard-static/issues"
	},
	"homepage": "https://github.com/georgebullock/react-interactive-dashboard-static#readme",
	"devDependencies": {
		"@babel/core": "^7.8.6",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-env": "^7.8.6",
		"@babel/preset-react": "^7.8.3",
		"@babel/preset-typescript": "^7.9.0",
		"@testing-library/jest-dom": "^5.1.1",
		"@testing-library/react": "^10.0.1",
		"@types/babel__core": "^7.1.7",
		"@types/babel__preset-env": "^7.9.0",
		"@types/clean-webpack-plugin": "^0.1.3",
		"@types/eslint": "^6.8.0",
		"@types/html-webpack-plugin": "^3.2.2",
		"@types/jest": "^25.2.1",
		"@types/mini-css-extract-plugin": "^0.9.1",
		"@types/node-sass": "^4.11.0",
		"@types/prettier": "^2.0.0",
		"@types/react": "^16.9.34",
		"@types/react-dom": "^16.9.6",
		"@types/testing-library__jest-dom": "^5.0.3",
		"@types/testing-library__react": "^10.0.1",
		"@types/webpack": "^4.41.10",
		"@types/webpack-dev-server": "^3.10.1",
		"@typescript-eslint/eslint-plugin": "^2.27.0",
		"@typescript-eslint/parser": "^2.27.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"cross-env": "5.0.5",
		"css-loader": "^3.4.2",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-jest-dom": "^2.0.1",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.19.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^4.2.5",
		"jest": "^25.1.0",
		"lint-staged": "^10.2.11",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.13.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.19.1",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.3",
		"ts-jest": "^25.3.1",
		"typescript": "^3.8.3",
		"webpack": "^4.42.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3"
	},
	"dependencies": {
		"react": "^16.13.0",
		"react-dom": "^16.13.0"
	}
}
